<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:helpers="using:TimeCafe.UI.Utilities.Converters">

    <helpers:StringToVisibilityConverter x:Key="StringToVisibilityConverter" />
    <helpers:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <helpers:BooleanNegationConverter x:Key="BooleanNegationConverter" />
    <helpers:BooleanToTextConverter x:Key="BooleanToTextConverter" />
    <helpers:TooltipConverter x:Key="TooltipConverter" />
    <helpers:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <helpers:StatusColorConverter x:Key="StatusColorConverter" />
    <helpers:StatusToVisibilityConverter x:Key="StatusToVisibilityConverter" />
    <helpers:FullNameConverters x:Key="FullNameConverters" />
    <helpers:DateOnlyConverter x:Key="DateOnlyConverter" />
    <helpers:FullNameInitialsConverters x:Key="FullNameInitialsConverters" />
    <helpers:DateTimeFormatConverter x:Key="DateTimeFormatConverter" />
    <helpers:GenderConverters x:Key="GenderConverters" />
    <helpers:DateOnlyToDateTimeConverter x:Key="DateOnlyToDateTimeConverter" />
    <FontFamily x:Key="VAGRoundCyrillic">/Assets/Fonts/vagroundcyrillic.ttf#VAG Round Cyrillic</FontFamily>
    <helpers:ThemeToBrushConverter x:Key="ThemeToBrushConverter" />
    <helpers:ThemeToStyleConverter x:Key="ThemeToStyleConverter" />
    <helpers:PriceConverter x:Key="PriceConverter" />
    <helpers:DecimalToDoubleConverter x:Key="DecimalToDoubleConverter" />
